class Solution:
    def findErrorNums(self, nums: List[int]) -> List[int]:
        n = len(nums)
        seen = set()
        duplicate = -1
        total = 0

        for x in nums:
            total += x
            if x in seen:
                duplicate = x
            else:
                seen.add(x)

        expected = n * (n + 1) // 2
        missing = expected - (total - duplicate)
        return [duplicate, missing]
